{
    "name": "qlimix/time",
    "type": "library",
    "description": "An interface to lapse time",
    "license": "MIT",
    "keywords": [
        "time",
        "lapse"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
